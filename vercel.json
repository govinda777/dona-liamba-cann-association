{
  "buildCommand": "pnpm build --filter=frontend",
  "outputDirectory": "apps/frontend/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "apps/frontend/src/app/api/**": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "regions": ["gru1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
